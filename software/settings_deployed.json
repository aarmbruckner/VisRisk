{
  "public": {
    "appInfo":
    {
      "version" : "0.9.1",
      "serverUrl":"http://127.0.0.1:3000",
      "httpsServerUrl":"https://127.0.0.1:3430",
      "httpsPort":3430,
      "httpPort":3000
    },
    "mail":
    {
      "tokenMailAdress":"riskestimationrecorder@riskest.com"
    },
    "logo":
    {
      "logoLocation":"img/logo.png",
      "logoWidth":162,
      "logoHeight":30
    },
    "logging":{
      "logFileDirectory": ".\\log"
    }, 
    "localisation":
    {
      "i18nLanguage" : "en-Us",
      "localTimeZoneOffset":2
    },
    "graphEditor":
    {
      "editorTemplate":"graphTemplates/editorTemplate.xml"
    },
    "surveyEditor":
    {
      "clearGraphAfterLogSave": false
    },
    "defaultGraphConfigurations":
    {
      "calcConfig":
      {
        "opinionPooling":
        {
          "decimalPointAccuracy":5,
          "epsilon":0.1
        },
        "dataCorrection":
        { 
          "pX":0.2,
          "pY":0.2,
          "manualDeltaEnabled":false,
          "deltaXLow":0.1,
          "deltaXHigh":0.1,
          "deltaYLow":0.1,
          "deltaYHigh":0.1
        }
      },
      "gridConfig":
      {
        "xAxis":
        {
          "name":"Likelihood",
          "borders":"true",
          "level1":
          {
            "name":"Low",
            "description":"Incident could occur, less than 10% likelihood in projects like this."
          },
          "level2":
          {
            "name":"Medium",
            "description":"Likely that it will happen several times per year, 10-50% likelihood in projects like this."
          },
          "level3":
          {
            "name":"High",
            "description":"Likely that it will happen several times per year, more than 50% likelihood in projects like this."
          },
          "border1":"10%",
          "border2":"50%"
        },
        "yAxis":
        {
          "name":"Impact",
          "borders":"true",
          "level1":
          {
            "name":"Low",
            "description":"A low impact such that no review would be required by any project stakeholder."
          },
          "level2":
          {
            "name":"Medium",
            "description":"Moderate impact to the project scope, schedule, cost or quality, that would require explicit review and approval by the project manager."
          },
          "level3":
          {
            "name":"High",
            "description":"Significant change to the project scope, schedule, cost or quality."
          },
          "border1":"10.000",
          "border2":"100.000"
        }
      },
      "networkConfig":
      {
        "network1":
        {
          "node1":
          {
            "name":"powerplant",
            "failureProbability":0.0
          },
          "node2":
          {
            "name":"watersupply",
            "failureProbability":0.0,
            "providerDependencies":{
              "providerDependencyInfo1":
              {
                "timeLimitLow":
                {
                  "unit":"hour",
                  "duration":12
                },
                "timeLimitMedium":
                {
                  "unit":"hour",
                  "duration":24
                },
                "probabilityLow":
                {
                  "value":0.2
                },
                "probabilityMedium":
                {
                  "value":0.4
                },
                "probabilityHigh":
                {
                  "value":0.6
                }
              }
            }
          }
        }
      }
    }
  }
}
